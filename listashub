as", "Conversíveis", "Cupê Esportivo", "Detran ES", "Hatches", "Minivan", "Picapes", "SUV", "Sedans", "Station Wagon & Peruas", "Sucata de Carros", "Utilitários & Furgões", "Veículos Sinistrados"], "Carros de Detran": ["Ambulâncias", "Combo de Carros", "Cupê Esportivo", "Hatches", "Picapes", "SUV & Jipes", "SW Peruas", "Sedans", "Sucata de Carros", "Utilitários & Furgões", "Veículos Sinistrados"], "Coleções": ["GM Chevrolet", "Mercedes Benz", "Porche"], "Consórcios Autos": ["Consórcio autos", "Contemplados", "Cotas"], "Conversíveis": ["BMW", "Jaguar", "Mini", "Porche"], "Cupê Esportivo": ["Jeep"], "Hatches": ["Audi", "BMW", "Chery", "Chrysler", "Citroen", "Fiat", "Ford", "GM Chevrolet", "Honda", "JAC", "Kia", "Nissan", "Peugeot", "Renault", "Toyota", "VW Volkswagen"], "Jipes e Crossovers": ["Citroen", "GM Chevrolet", "Honda", "Ssangyong", "Toyota", "Troller", "VW Volkswagen"], "Partes e peças - Carros e motos": ["Asiento"], "Picapes": ["Agrale", "Changan", "Fiat", "Ford", "GM Chevrolet", "Hafei", "Hyundai", "Kia", "Mercedes benz", "Mitsubishi", "Nissan", "Toyota", "VW Volkswagen"], "Sedans": ["Audi", "BMW", "Chery", "Chrysler", "Citroen", "Fiat", "Ford", "GM Chevrolet", "Honda", "Hyundai", "JAC", "Lifan", "Mercedes benz", "Nissan", "Peugeot", "Renault", "Toyota", "VW Volkswagen"], "Sucatas": ["Peças e Componentes", "Sucata de Carros", "Sucata de Motos"], "SUV": ["BMW", "Citroen", "Dodge", "Ford", "GM Chevrolet", "Hyundai", "Jeep", "Kia", "Land Rover", "Mitsubishi", "Renault", "Suzuki", "Toyota", "VW Volkswagen", "Volvo"], "SW Peruas": ["Chrysler", "Citroen", "Fiat", "Ford", "GM Chevrolet", "Hyundai", "Mercedes benz", "Peugeot", "Renault", "Subaru", "Toyota", "VW Volkswagen"], "Utilitários": ["Changan", "Fiat", "Ford", "GM Chevrolet", "Hafei", "Kia", "Lifan", "Mercedes Benz", "Mini", "Mitsubishi", "Renault", "VW Volkswagen", "Volvo"]}}</script>

  <div class="app">
    <div class="card">
      <h1>Construtor de Filtros – HubSpot (simplificado v2)</h1>
      <div id="integrity" class="muted" style="margin-bottom:8px;"></div>

      <div class="row">
        <div>
          <label for="mgmt">Management (DESC_MANAGEMENT_CATEGORY_BIDPARTS)</label>
          <select id="mgmt"><option value="">Selecione…</option></select>
        </div>
        <div>
          <label for="behavior">Comportamento (funil)</label>
          <select id="behavior">
            <option value="">(Nenhum)</option>
            <option value="Visualizou">Visualizou</option>
            <option value="Habilitou">Habilitou-se (HP)</option>
            <option value="Lance">Lance</option>
            <option value="Contemplado">Contemplado</option>
          </select>
        </div>
      </div>

      <div class="row">
        <div>
          <label for="category">Category (DESC_CATEGORY)</label>
          <select id="category" disabled><option value="">Selecione…</option></select>
        </div>
        <div>
          <label for="subcategory">Subcategory (DESC_SUBCATEGORY)</label>
          <select id="subcategory" disabled><option value="">Selecione…</option></select>
        </div>
      </div>

      <label for="clusterType">Clusterização de Compradores</label>
      <div style="display:grid; grid-template-columns:1fr 1fr; gap:10px; margin-bottom:12px;">
        <select id="clusterType">
          <option value="">Sem Cluster</option>
          <option value="recorrencia_habilitacao">Recorrência de habilitação</option>
          <option value="recorrencia_compra">Recorrência de compra</option>
        </select>
        <input id="clusterMin" type="number" min="0" step="1" placeholder="Mínimo (ex.: 3)">
      </div>

      <div class="actions">
        <button id="generate" class="primary">Gerar</button>
        <button id="copy" class="secondary">Copiar</button>
        <button id="reset" class="secondary">Limpar</button>
        <label class="muted">Modo:</label>
        <select id="mode" style="max-width:200px;">
          <option value="step">Passo-a-passo</option>
          <option value="advanced">Avançado</option>
        </select>
      </div>
    </div>

    <div class="card">
      <h2>Resultado</h2>
      <div class="muted">No <b>Passo-a-passo</b>, copie os passos e aplique no HubSpot. No <b>Avançado</b>, você vê a versão técnica.</div>
      <div id="resultStep" class="output" style="display:block;"></div>
      <div id="resultAdv" class="output" style="display:none;"></div>
      <div class="row" style="margin-top:12px;">
        <div class="card"><h2>Resumo</h2><div id="summary" class="muted"></div></div>
        <div class="card"><h2>Pseudo (JSON)</h2><div id="pseudo" class="output" style="min-height:100px;"></div></div>
      </div>
    </div>
  </div>

<script>
(function(){
  var DATA = {}
  try {
    DATA = JSON.parse(document.getElementById('data-json').textContent);
  } catch(e) { console.error("Erro ao ler DATA", e); }

  var MGMT_PROP='DESC_MANAGEMENT_CATEGORY_BIDPARTS';
  var CAT_PROP='DESC_CATEGORY';
  var SUB_PROP='DESC_SUBCATEGORY';
  var BU_PROP='BUSINESS_UNIT';
  var BU_VALUE='Brasil';

  var mgmtEl=document.getElementById('mgmt');
  var behaviorEl=document.getElementById('behavior');
  var catEl=document.getElementById('category');
  var subEl=document.getElementById('subcategory');
  var clusterTypeEl=document.getElementById('clusterType');
  var clusterMinEl=document.getElementById('clusterMin');
  var resultStepEl=document.getElementById('resultStep');
  var resultAdvEl=document.getElementById('resultAdv');
  var modeEl=document.getElementById('mode');
  var pseudoEl=document.getElementById('pseudo');
  var summaryEl=document.getElementById('summary');
  var integrityEl=document.getElementById('integrity');

  function opt(v,t){var o=document.createElement('option');o.value=v;o.textContent=t;return o;}
  function resetSelect(el,ph){el.innerHTML='';el.appendChild(opt('',ph||'Selecione…'));el.disabled=true;}
  function fillSelect(el,arr,addAll){resetSelect(el);if(addAll) el.appendChild(opt('__ALL__','Todos'));arr.forEach(function(x){el.appendChild(opt(x,x));});el.disabled=false;}

  // Populate mgmt
  var mgmts=Object.keys(DATA).sort(function(a,b){return a.localeCompare(b);});
  mgmts.forEach(function(m){mgmtEl.appendChild(opt(m,m));});
  try{
    var mgmtCount=mgmts.length;
    var catCount=mgmts.reduce((acc,m)=>acc+Object.keys(DATA[m]).length,0);
    var subCount=mgmts.reduce((acc,m)=>acc+Object.values(DATA[m]).reduce((a,arr)=>a+arr.length,0),0);
    integrityEl.innerHTML = "Dados carregados: <b>"+mgmtCount+"</b> managements · <b>"+catCount+"</b> categorias · <b>"+subCount+"</b> subcategorias.";
  }catch(e){ integrityEl.textContent="Erro ao processar dados"; }

  mgmtEl.addEventListener('change', function(){
    resetSelect(catEl,'Selecione…'); resetSelect(subEl,'Selecione…');
    var m=mgmtEl.value; if(!m) return;
    var cats=Object.keys(DATA[m]||{}).sort(function(a,b){return a.localeCompare(b);});
    fillSelect(catEl,cats,true);
  });

  catEl.addEventListener('change', function(){
    resetSelect(subEl,'Selecione…');
    var m=mgmtEl.value, c=catEl.value; if(!m||!c) return;
    if(c==='__ALL__'){ subEl.disabled=true; return; }
    var subs=DATA[m][c]||[]; fillSelect(subEl,subs,true);
  });

  document.getElementById('reset').addEventListener('click', function(){
    mgmtEl.value=''; behaviorEl.value=''; clusterTypeEl.value=''; clusterMinEl.value='';
    resetSelect(catEl,'Selecione…'); resetSelect(subEl,'Selecione…');
    resultStepEl.textContent=''; resultAdvEl.textContent=''; pseudoEl.textContent=''; summaryEl.textContent='';
  });

  modeEl.addEventListener('change', function(){
    var mode=modeEl.value; if(mode==='step'){resultStepEl.style.display='block';resultAdvEl.style.display='none';}
    else{resultStepEl.style.display='none';resultAdvEl.style.display='block';}
  });

  function buildSummary(m,b,c,s,clusterType,clusterMin){
    var parts=[];
    if(m) parts.push("Management: **"+m+"**");
    if(c) parts.push("Category: **"+(c==='__ALL__'?'Todos':c)+"**");
    if(s) parts.push("Subcategory: **"+(s==='__ALL__'?'Todos':s)+"**");
    if(b) parts.push("Comportamento: **"+b+"**");
    if(clusterType){
      var label = clusterType==='recorrencia_habilitacao'?'Recorrência de habilitação':'Recorrência de compra';
      var suf = clusterMin?(" ≥ "+clusterMin):"";
      parts.push("Cluster: **"+label+suf+"**");
    }
    return parts.join(" · ");
  }

  function buildPseudo(m,b,c,s,clusterType,clusterMin){
    var clauses=[];
    if(m) clauses.push({property:MGMT_PROP,operator:"EQ",value:m});
    if(c && c!=='__ALL__') clauses.push({property:CAT_PROP,operator:"EQ",value:c});
    if(s && s!=='__ALL__') clauses.push({property:SUB_PROP,operator:"EQ",value:s});
    if(clusterType) clauses.push({cluster:{type:clusterType,operator:"GTE",min:(clusterMin?parseInt(clusterMin):null)}});
    var behaviorClause=b?{behavior:b}:null;
    return JSON.stringify({all:clauses,behavior:behaviorClause},null,2);
  }

  function buildInstruction(m,b,c,s,clusterType,clusterMin){
    var lines=[];
    lines.push("**Criação da Lista no HubSpot (UI)**","");
    lines.push("1) Vá em **CRM → Segmentos (listas) → Criar lista**.");
    lines.push("2) Escolha **Baseada em contatos** e **Lista ativa**.","");
    lines.push("3) **Defina o escopo por Entity (objeto associado):**");
    lines.push("   • Selecione: **Entity (objeto associado)**.");
    lines.push("   • **Contato está associado a** → **qualquer Entity**.","");
    lines.push("4) **Dentro do Entity associado, aplique os filtros de categoria:**");
    var mgmtLabel=m||"Caminhões";
    var behavPhrase=b?("é qualquer um de **"+(b==="Habilitou"?"Habilitou-se (HP) ou Visualizou":b==="Visualizou"?"Visualizou ou Habilitou-se (HP)":b)+"**"):"selecione o comportamento";
    lines.push("   • **Propriedade "+mgmtLabel+".** "+behavPhrase+".");
    var catText=(c&&c!=='__ALL__')?c:"Caminhões Basculantes";
    lines.push("   • **Depois aplique Entity associado (Categoria de maior interesse) = "+catText+".**");
    var subText=(s&&s!=='__ALL__')?s:"Agrale";
    lines.push("   • **Depois aplique Entity associado (Subcategoria de maior interesse) = "+subText+".**");
    if(clusterType){
      var label = clusterType==='recorrencia_habilitacao'?'Recorrência de habilitação':'Recorrência de compra';
      var suf = clusterMin?(" ≥ "+clusterMin):"";
      lines.push("   • **Depois aplique Entity associado "+label+suf+".**");
    }
    lines.push("   • **Depois aplique Entity associado BUSINESS_UNIT = Brasil.**","");
    lines.push("5) **Nome sugerido da lista**:");
    var nameParts=["MKT"];
    if(m) nameParts.push("| Management: "+m);
    if(c) nameParts.push("| Cat: "+(c==='__ALL__'?'Todos':c));
    if(s) nameParts.push("| Sub: "+(s==='__ALL__'?'Todos':s));
    if(clusterType){
      var label = clusterType==='recorrencia_habilitacao'?'Habilitação':'Recorrência';
      var suf = clusterMin?(" ≥ "+clusterMin):"";
      nameParts.push("| Cluster: "+label+suf);
    }
    if(b) nameParts.push("| Behav: "+b);
    lines.push("   • **"+nameParts.join(" ")+"**");
    lines.push("","6) Salve.");
    return lines.join("\n");
  }

  document.getElementById('generate').addEventListener('click', function(){
    var m=mgmtEl.value,b=behaviorEl.value,c=catEl.value,s=subEl.value;
    var clusterType=clusterTypeEl.value,clusterMin=clusterMinEl.value;
    if(!m){resultStepEl.innerHTML="Selecione Management.";return;}
    var summary=buildSummary(m,b,c,s,clusterType,clusterMin);
    summaryEl.innerHTML=summary.replace(/\*\*(.*?)\*\*/g,"<b>$1</b>");
    var pseudo=buildPseudo(m,b,c,s,clusterType,clusterMin);
    pseudoEl.textContent=pseudo;
    var instructions=buildInstruction(m,b,c,s,clusterType,clusterMin);
    if(modeEl.value==='step'){resultStepEl.innerHTML=instructions.replace(/\n/g,"<br>").replace(/\*\*(.*?)\*\*/g,"<b>$1</b>");resultStepEl.style.display='block';resultAdvEl.style.display='none';}
    else{resultAdvEl.innerHTML=instructions.replace(/\n/g,"<br>").replace(/\*\*(.*?)\*\*/g,"<b>$1</b>");resultAdvEl.style.display='block';resultStepEl.style.display='none';}
  });
})();
</script>
</body>
</html>
